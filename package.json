    "analyze": "yarn build --analyze",
    "lingui": "lingui",
    "test": "craco test",
    "eject": "craco eject",
    "schema": "apollo schema:download --endpoint=https://s-staging.widgetbot.io/api/graphql src/lib/apollo/codegen/schema.json",
    "generate": "yarn schema && yarn codegen && yarn codegen:tasks",
    "codegen:tasks": "ts-node --skip-project src/lib/apollo/codegen/tasks",
    "codegen": "apollo codegen:generate --target=typescript --addTypename --outputFlat src/__generated__",
    "codegen:live": "apollo codegen:generate --target=typescript --addTypename --outputFlat src/__generated__ --schema=\"https://s-staging.widgetbot.io/api/graphql\"",
    "typecheck": "tsc --noEmit",
    "doc": "typedoc --includeDeclarations --excludeExternals --externalPattern \"**/node_modules/!(@widgetbot)/**\" --ignoreCompilerErrors --out ./docs/ ./src"
  },
